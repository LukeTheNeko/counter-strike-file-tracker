{
    "material-icon-theme.files.associations": {
      "settings.json": "Raml",
      "*.lang.json": "i18n",
      "lang.json": "i18n",
      "gitignore": "Git"
    },
    "material-icon-theme.folders.theme": "specific",
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
      ".env": ".env.development, .env.production, .env.test, .env.example, .env.dev",
      "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, tsconfig.json, .eslintrc.json, .gitignore, LICENSE",
    },
    "typescript.suggest.autoImports": true,
    "window.title": "${rootName}${separator}${appName}",
    "files.autoSave": "afterDelay",
  }